{
    "manifest_version": 3,
    "name": "Bible Extension",
    "description": "For Katherine and Alicia. Group Bible in one extension.",
    "version": "1.0",
    "action": {
      "default_popup": "/popup/popup.html",
      "default_icon": "/images/icon.png"
    },
    "content_scripts": [ {
      "matches": ["<all_urls>"],
      "js": ["/popup/popup.js"]
    } ]
}